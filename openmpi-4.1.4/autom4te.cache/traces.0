    AS_IF([test "$DISABLE_$2" = "1"], [want_component=0])
    AS_LITERAL_IF([$3],
        [AS_IF([test "$DISABLE_$2_$3" = "1"], [want_component=0])],
        [str="DISABLED_COMPONENT_CHECK=\$DISABLE_$2_$3"
         eval $str
